#content-main-container
  #content-main-wide
    %h2=@title
    - form_for @user_session, :url => { :controller => 'sessions' }, :method => 'post' do |f|
      = f.error_messages
      %p
        = f.label :email
        = f.text_field :primary_email
      %p
        = f.label :password
        = f.password_field :password
    
      %p= f.submit "login"
